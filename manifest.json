{
  "manifest_version": 2,
  "name": "Colored Tabs",
  "version": "1.0",

  "description": "Add custom colours / icons to pinned tabs",

  "icons": {
    "32": "icons/icons8-pin-32.png",
    "48": "icons/icons8-pin-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["colorise-tabs.js"]
    }
  ]
}
